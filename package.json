{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev-ext": "web-ext run --source-dir=public --artifacts-dir=artifacts",
    "build-ext": "web-ext build --source-dir=public --artifacts-dir=artifacts --overwrite-dest",
    "build-all": "npm run build && npm run build-ext"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "web-ext": "^7.4.0",
    "web-ext-types": "^3.2.1"
  },
  "dependencies": {
    "sass": "^1.57.1",
    "sirv-cli": "^2.0.2",
    "svelte-material-icons": "^2.0.4"
  }
}
